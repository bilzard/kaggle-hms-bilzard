    "from scipy.optimize import fsolve\n",
    "\n",
    "\n",
    "def func(x, y):\n",
    "    return 1.09 - 1.64 * x + 1.28 * x**2 - 0.373 * x**3 - y\n",
    "\n",
    "\n",
    "y_values = [0.6, 0.55, 0.65, 0.58, 0.67]\n",
    "\n",
    "for y in y_values:\n",
    "    x_guess = 0\n",
    "    x_solution = fsolve(func, x_guess, args=(y))\n",
    "    print(f\"y = {y}: x ≈ {x_solution[0]}\")"
   ]
  }
